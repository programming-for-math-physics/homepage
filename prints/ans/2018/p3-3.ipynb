   "source": [
    "def integrate(f, a, b, n):\n",
    "    s = 0.0\n",
    "    dx = (b - a) / n\n",
    "    x = a\n",
    "    for i in range(n):\n",
    "        s = s + f(x) * dx\n",
    "        x = x + dx\n",
    "    return s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
