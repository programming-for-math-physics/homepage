   "source": [
    "def prod(a, b):\n",
    "    p = 1\n",
    "    for x in range(a, b+1):\n",
    "        p = p * x\n",
    "    return p\n",
    "\n",
    "def ncr(n, r):\n",
    "    return prod(1, n) / (prod(1, r) * prod(1, n - r))\n",
    "\n",
    "def a(n):\n",
    "    return ncr(2 * n + 1, n) / prod(1, n)\n",
    "print(a(1))\n",
    "print(a(2))\n",
    "print(a(3))\n",
    "print(a(4))\n",
    "print(a(5))"
   ]
  }
